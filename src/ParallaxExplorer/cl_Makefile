include_mqtt = -lpaho-mqtt3c -lpaho-mqtt3a -lpaho-mqttpp3
Explorer: main.o Messages.o
	g++ -o ../ParallaxExplorer.exe build/main.o build/Messages.o ${include_mqtt}

Messages.o: mkdirBuild
	g++ -o build/Messages.o -c mqttMsgStruct/Messages.cpp

main.o: mkdirBuild
	g++ -o build/main.o -c main.cpp

mkdirBuild:
	mkdir -p build
